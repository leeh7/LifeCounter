<?xml version="1.1" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.davidlee.lifecounter"
    android:versionCode="2"
    android:versionName="1.1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/lifecountericon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="me.davidlee.lifecounter.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="me.davidlee.lifecounter.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="me.davidlee.lifecounter.CounterActivity"
            android:label="@string/title_counter_settings" >
        </activity>
    </application>

</manifest>
